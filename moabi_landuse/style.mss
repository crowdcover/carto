#landuse{
  [occupation = 'Foret']{
  polygon-fill: #487427;
  polygon-opacity: 1;
  line-width:0;
  line-color:#444343;
  line-opacity:0;  
  }
  
  [occupation = 'Mosaique']{
  polygon-fill: #859f68;
  polygon-opacity: 1;
  line-width:0;
  line-color:#444343;
  line-opacity:0;  
  }
  
  [occupation = 'Savane']{
  polygon-fill: #bdc177;
  polygon-opacity: 1;
  line-width:0;
  line-color:#444343;
  line-opacity:0;  
  }
  
  [occupation = 'Habitation']{
  polygon-fill: #d70e0e;
  polygon-opacity: 1;
  line-width:0;
  line-color:#444343;
  line-opacity:0;  
  }
  
  [occupation = 'Agricole']{
  polygon-fill: #91b64b;
  polygon-opacity: 1;
  line-color:#444343;
  line-opacity:0; 
  [zoom <= 8]{polygon-pattern-file: url('../img/stripe_m_gray_#999999/ex_small.png');}
  [zoom > 8]{polygon-pattern-file: url('../img/stripe_m_gray_#999999/ex_small.png');}
  }
  
}


